<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <listener>
        <listener-class>com.tp.config.InitialisationDaoFactory</listener-class>
    </listener>

    <servlet>
        <servlet-name>Liste_etu</servlet-name>
        <servlet-class>com.tp.servlets.Liste_etu</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Liste_etu</servlet-name>
        <url-pattern>/liste</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Create_etu</servlet-name>
        <servlet-class>com.tp.servlets.Create_etu</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Create_etu</servlet-name>
        <url-pattern>/nouvel_etu</url-pattern>
    </servlet-mapping>
</web-app>